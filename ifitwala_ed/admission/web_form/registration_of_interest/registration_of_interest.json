 "client_script": "// apps/ifitwala_ed/ifitwala_ed/admission/web_form/registration_of_interest/registration_of_interest.js\n// --------------------------------------------------------------------\nconsole.log(\"\u2261 RoI script LOADED\");\n\nfunction attach_academic_year_filter() {\n\t// Control object becomes available only after Web-Form has rendered\n\tconst ctrl = frappe.web_form.get_field(\"proposed_academic_year\");\n\n\tif (!ctrl) {\n\t\tconsole.log(\"[RoI] proposed_academic_year ctrl not yet ready \u2192 retry in 100 ms\");\n\t\tsetTimeout(attach_academic_year_filter, 100);\n\t\treturn;\n\t}\n\n\tconsole.log(\"[RoI] ctrl found \u2192 attaching df.get_query filter\");\n\n\t// Link autocomplete calls df.get_query on every search\n\tctrl.df.get_query = () => {\n\t\tconst today   = frappe.datetime.get_today();\n\t\tconst filters = [[\"Academic Year\", \"year_end_date\", \">=\", today]];\n\t\tconsole.log(\"[RoI] get_query invoked \u2013 return\", filters);\n\t\treturn { filters };\n\t};\n\n\t// Optional: force the control to pick up the new get_query immediately\n\tif (ctrl.$input) ctrl.$input.val(\"\");       // clear current value\n}\n\nfrappe.ready(() => {\n\tconsole.log(\"[RoI] frappe.ready fired \u2013 starting filter-attach sequence\");\n\tattach_academic_year_filter();\n});\n",

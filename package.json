{
  "name": "ifitwala_ed",
  "description": "An Open Source Education ERP System powered by the Frappe Framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/fderyckel/ifitwala_ed.git"
  },
  "homepage": "https://ifitwala.com",
  "author": "Fran√ßois de Ryckel",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/fderyckel/ifitwala_ed/issues"
  },
  "scripts": {
    "build": "yarn build:desk && yarn build:spa",
    "build:desk": "NODE_ENV=production rollup -c",
    "build:spa": "yarn --cwd ifitwala_ed/ui-spa install --frozen-lockfile --check-files && yarn --cwd ifitwala_ed/ui-spa build",
    "type-check:spa": "yarn --cwd ifitwala_ed/ui-spa type-check",
    "bench-build": "bench build",
    "hierarchy": "rollup -c --watch --configPlugin rollup-plugin-postcss --environment WATCH_HIERARCHY"
  },
  "devDependencies": {
    "@fullcalendar/core": "6.1.20",
    "@fullcalendar/daygrid": "6.1.20",
    "@fullcalendar/list": "6.1.20",
    "@fullcalendar/timegrid": "6.1.20",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "postcss": "^8.5.4",
    "postcss-import": "^16.1.1",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.97.1",
    "tailwindcss": "^4.1.18"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.13.1",
    "frappe-ui": "^0.1.243",
    "html2canvas": "1.4.1",
    "onscan.js": "^1.5.2"
  }
}
